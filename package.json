{
  "name": "d.o.it.brainstorming",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix --cache",
    "ci:lint": "eslint src --ext .ts,.tsx --max-warnings=0",
    "ci:lint:lenient": "eslint src --ext .ts,.tsx --max-warnings=100 --no-error-on-unmatched-pattern",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "aws-sdk": "^2.1584.0",
    "openai": "^4.35.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "reactflow": "^11.10.4",
    "uuid": "^9.0.1",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.1"
  }
}
