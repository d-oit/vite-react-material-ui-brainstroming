{
  "name": "d.o.it.brainstorming",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix --cache --max-warnings=0",
    "lint:warn": "eslint . --cache",
    "lint:check": "eslint . --cache --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "lint:fix-all": "eslint . --fix --quiet --rule \"@typescript-eslint/no-unused-vars: off\" --rule \"@typescript-eslint/no-explicit-any: off\"",
    "ci:lint": "eslint . --max-warnings=0 --no-fix",
    "lint:staged": "lint-staged --concurrent false",
    "ci:verify": "npm run typecheck && npm run ci:lint",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:serial": "vitest run --config vitest.config.ts --pool=threads --poolOptions.threads.singleThread --maxConcurrency=1",
    "test:debug": "vitest run --config vitest.config.ts --pool=threads --poolOptions.threads.singleThread --maxConcurrency=1 --testNamePattern",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "playwright test src/tests/accessibility/a11y-audit.ts",
    "test:a11y:report": "playwright test src/tests/accessibility/a11y-audit.ts --reporter=html && open playwright-report/index.html",
    "prepare": "husky install",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@types/dagre": "^0.7.52",
    "aws-sdk": "^2.1584.0",
    "buffer": "^6.0.3",
    "dagre": "^0.8.5",
    "framer-motion": "^12.6.3",
    "i18next": "^25.0.2",
    "openai": "^4.35.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.5.1",
    "react-router-dom": "^7.5.2",
    "reactflow": "^11.10.4",
    "stylis-plugin-rtl": "^2.1.1",
    "use-debounce": "^10.0.4",
    "uuid": "^9.0.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "workbox-window": "^7.0.0",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{json,css,md}": [
      "eslint --fix --max-warnings=0"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.5.0",
    "sort-package-json": "^2.8.0",
    "typescript": "^5.4.3",
    "vite": "^6.2.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.1"
  }
}
